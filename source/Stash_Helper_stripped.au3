Local $aPos = WinGetPos($hWnd)
If Not IsArray($aPos) Then Return SetError(-1, -1, False)
Local $tPoint = DllStructCreate($tagPOINT)
DllStructSetData($tPoint, "X", $aPos[0])
DllStructSetData($tPoint, "Y", $aPos[1])
