$iResult = $_WD_ERROR_SocketError
Else
Switch $aURL[1]
Case $INTERNET_SCHEME_HTTP
$sResponseText = _WinHttpSimpleRequest($hConnect, "GET", $aURL[6] & $aURL[7], Default, Default, $_WD_HTTPContentType)
