_WinHttpSetTimeouts($hOpen, $_WD_HTTPTimeOuts[0], $_WD_HTTPTimeOuts[1], $_WD_HTTPTimeOuts[2], $_WD_HTTPTimeOuts[3])
EndIf
Local $hConnect = _WinHttpConnect($hOpen, $aURL[2], $aURL[3])
